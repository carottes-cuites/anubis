{
    "name": "anubis-bot",
    "version": "0.0.2",
    "description": "Anubis bot is an audio provider interface to get your favorite artists data, tracks, ... from any kind of services",
    "main": "index.js",
    "environment": "dev",
    "scripts": {
        "build_to_release": "cp -R build/app.js release/app.js && cp -R resources release/ && cp package.json release/package.json",
        "prod_build_patch": "NODE_ENV=production webpack --env.patch --progress --mode production",
        "prod_build_minor": "NODE_ENV=production webpack --env.minor --progress --mode production",
        "prod_build_major": "NODE_ENV=production webpack --env.major --progress --mode production",
        "prod_build_release_patch": "npm run \"prod_build_patch\" && npm run \"build_to_release\"",
        "prod_build_release_minor": "npm run \"prod_build_minor\" && npm run \"build_to_release\"",
        "prod_build_release_major": "npm run \"prod_build_major\" && npm run \"build_to_release\"",
        "dev_build": "webpack --mode development",
        "dev_build_release": "npm run \"dev_build\" && npm run \"build_to_release\"",
        "start": "node ./src/index.js",
        "start_release": "node ./release/app.js",
        "start_build": "node ./build/app.js",
        "start_app": "echo Launching app... && node app.js",
        "start_app_refresh": "echo Refreshing node_modules... && rm -rf node_modules && npm install && echo Launching app... && node app.js",
        "test": "echo \"Error: no test specified\" && exit 1",
        "watch": "webpack --watch"
    },
    "keywords": [
        "music",
        "anubis"
    ],
    "author": "jdarras",
    "license": "ISC",
    "dependencies": {
        "bufferutil": "^3.0.4",
        "discord.js": "^11.3.2",
        "dropbox": "^2.5.13",
        "duplex-stream": "^0.1.0",
        "erlpack": "github:hammerandchisel/erlpack",
        "ffmpeg": "0.0.4",
        "fluent-ffmpeg": "^2.1.2",
        "fs": "0.0.1-security",
        "google-tts-api": "0.0.2",
        "googleapis": "^28.1.0",
        "i18n": "^0.8.3",
        "libsodium-wrappers": "^0.5.4",
        "node-opus": "^0.2.6",
        "promise.prototype.finally": "^3.1.0",
        "request": "^2.85.0",
        "request-promise": "^4.2.2",
        "twitch-get-stream": "^0.4.1",
        "ytdl-core": "^0.20.2"
    },
    "devDependencies": {
        "json-loader": "^0.5.7",
        "nodemon": "^1.17.3",
        "raw-loader": "^0.5.1",
        "uglifyjs-webpack-plugin": "^1.2.4",
        "val-loader": "^1.1.0",
        "webpack": "^4.5.0",
        "webpack-auto-inject-version": "^1.1.0",
        "webpack-cli": "^2.0.14",
        "webpack-dev-server": "^3.1.1"
    }
}